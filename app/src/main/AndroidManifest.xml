<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shrutislegion.sportify">

    <application
        android:allowBackup="true"
        android:exported = "true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sportify">
        <activity android:name=".LanderLogActivity"
            android:label="Register Your Complex"></activity>
        <activity android:name=".LenderHomeActivity" />
        <activity android:name=".AddComplexActivity"
            android:label="Add Your Complex"/>
        <activity android:name=".RegistrationActivity"
            android:label="Welcome to Sportify"/>
        <activity android:name=".LoginActivity"
            android:label="Sign In"/>
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>